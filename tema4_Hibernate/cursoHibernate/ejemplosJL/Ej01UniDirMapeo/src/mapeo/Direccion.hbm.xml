<hibernate-mapping>
    <class name="mapeo.Direccion" table="DIRECCIONES">
        <id name="id" column="ID"> 
            <generator class="identity" /> 
        </id> 
        <property name="calle" /> 
        <property name="codigoPostal" /> 
    </class>
</hibernate-mapping>
